puts([i8]) => i32

print(i32 i, i32 argc, [[i8]] argv) => i32
 = i >= argc ? 0
 : (
    puts(argv[i]),
    print(i + 1, argc, argv)
)

foo() => [] = (
    puts("foo"),
    $ => i32 = (puts("bar"), 0)
)

main(i32 argc, [[i8]] argv) => i32 = (
    print(0, argc, argv),
    foo() as [() => i32](),
    0
)
